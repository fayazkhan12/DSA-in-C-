// occurance of the number ..gfg podt
#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"enter the size of arrays = ";
    cin>>n;
    int arr[n];
    for(int i=0;i<n-1;i++)
    {
        cin>>arr[i];
        
    }
    int x ;
    int id=0;
    cout<<"enter the element you want  to cout ";
    cin>>x;
    
    for(int i=0;i<n-1;i++)
    {
        if(arr[i]==x) 
        {
         
         id++;
        }
    }
    cout<<id;
}
