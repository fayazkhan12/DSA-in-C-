#include<iostream>
#include<vector> 
using namespace std;
int main(){
    
    vector<int>s;
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    s.push_back(5);
    
    
    
    for(int i=0;i<s.size();i++)
    {
        cout<<s[i]<<" ";
    }
    cout<<endl;
    s.pop_back();
    s.pop_back();
     for(int i=0;i<s.size();i++)
    {
        cout<<s[i]<<" ";
    }
    cout<<endl;
    
    cout<<"size is "<<s.size()<<" "<<endl;
    cout<<"capacity is is "<<s.capacity()<<" ";
}



























