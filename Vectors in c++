#include<iostream>
#include<vector>

using namespace std;
int main()
{
   
    vector<int>v;
    v.push_back(9);
    v.push_back(7);
    v.push_back(5);
    v.push_back(6);
    v.push_back(82);
    v.push_back(977);
    v.push_back(74);
    v.push_back(4);
    
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i]<<" ";
    }
    cout<<endl;
    
    sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i]<<" ";
    }
    cout<<endl;
    
}

















































