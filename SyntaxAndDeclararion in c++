// TAKING INPUT AND PRINTING THE ANSWER BY ARRAYS..
#include<iostream>
using namespace std;
int main(){
    
    int n[10];
    
    
    for(int i=0;i<=9;i++)
    {
        cout<<"enter the element..";
        cin>>n[i];
    }
    
    for(int i=0;i<=9;i++)
    
    {
      
       cout<<"elements aree = "<<n[i]<<endl;; 
    }
}

























