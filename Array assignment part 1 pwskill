QUES 2->



// calculate the first and second max in an array

#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"enter the elements = ";
    cin>>n;
    int arr[n];
    
    for(int i=0;i<=n-1;i++)
    {
        cin>>arr[i];
        
    }
    
    int smax;
    smax=arr[0];
    for(int i=1;i<=n-1;i++)
    {
        if(smax<arr[i])
        {
            smax=arr[i];
        }
        
    }
     int secmax;
    secmax=arr[0];
    for(int i=1;i<=n-1;i++)
    {
        if(secmax<arr[i] && secmax!=arr[0])
        {
            secmax=arr[i];
        }
        
    }
    cout<<"max number is"<<smax<<endl;
     cout<<" second max number is"<<secmax<<endl;
}



QUES 3->


//CALCULATE THE MN VALUE OF ALL ELEMENT

#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"enter the number = ";
    cin>>n;
    
    int arr[n];
    for(int i=0;i<=n-1;i++)
    {
        cin>>arr[i];
        
    }
    
    int min=arr[0];
    for(int i =1; i<=n-1;i++)
    {
        if(min>arr[i])
        
        {
            min=arr[i];
            
            
        }
        
        
    }
    
    
    cout<<"min element in the array is = "<<min;
}



















QUES-> 4



// GIVEN AN ARRAY FIND THE DUPLICATE ELEMNT IN ARRAY

#include<iostream>
using namespace std;
int main()
{
   int n;
   cout<<"enter the  number = ";
   cin>>n;
   int arr[n];
   for(int i=0;i<=n-1;i++)
   {
       cin>>arr[i];
   }
   int x;
   cout<<"enter element = ";
   cin>>x;
   for(int i=0;i<=n-1;i++)
   {
       if(arr[i]==x)
       {
           cout<<"DUPLICATE elements present = "<<x<<endl;
       }
       else{
           cout<<"NO DUPLICATE ELEMENTS.."<<endl;
       }
      
   }
    
   
}













